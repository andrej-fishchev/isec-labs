<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="508.0" prefWidth="899.0" spacing="20.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.lab4.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Label text="Минимальная длина исходного текста" />
   <TextField fx:id="minLength" editable="false" prefHeight="25.0" prefWidth="232.0" text="30" />
   <Label text="Исходный текст:" />

   <TextArea fx:id="sourceArea" prefHeight="198.0" prefWidth="859.0" wrapText="true">
      <font>
         <Font size="14.0" />
      </font>
   </TextArea>
   <Label fx:id="invalidKey" prefHeight="17.0" prefWidth="871.0" text="Label" visible="false" />
   <Button fx:id="encodeSourceBtn" mnemonicParsing="false" onAction="#onEncodeHandler" text="Хэшировать" />

   <Label text="Хэшированный текст:" />

    <TextArea fx:id="encodedArea" editable="false" prefHeight="199.0" prefWidth="859.0" />
</VBox>
